{
  "name": "iron-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run check:src-node-builtins && npm run generate:exercise-seed && npm run validate:seed && npm run generate:seed-payload",
    "build": "vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "check": "npm run lint && prettier --check .",
    "check:src-node-builtins": "node scripts/check-node-builtins-in-src.mjs",
    "test": "vitest",
    "test:run": "vitest run",
    "prepare": "git config core.hooksPath .githooks || true",
    "preview": "vite preview",
    "generate:exercise-seed": "node scripts/generate-exercise-seed.mjs",
    "validate:seed": "node scripts/validate-seed.mjs",
    "generate:seed-payload": "node scripts/generate-seed-payload.mjs",
    "seed:freeex": "node scripts/free-exercise-db.seed.mjs",
    "seed:freeex:images": "node scripts/free-exercise-db.seed.mjs --images",
    "seed:validate": "node scripts/validate-seed.mjs --inapp",
    "shipmain": "bash scripts/ship.sh",
    "prmain": "bash scripts/prmain.sh"
  },
  "dependencies": {
    "dexie-react-hooks": "^4.2.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fake-indexeddb": "^6.2.5",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}
