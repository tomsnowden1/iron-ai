# Server-only OpenAI key (never prefix with VITE_)
OPENAI_API_KEY=your-openai-api-key

# Safety guard: keep false unless you intentionally want Coach in production
ALLOW_COACH_PROD=false

# Client-safe mode flag (mode only, not a secret)
# server = default testing mode (uses server OPENAI_API_KEY)
# user = enables BYOK input in Settings
VITE_COACH_KEY_MODE=server

